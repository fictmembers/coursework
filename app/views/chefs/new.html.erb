<div class="formfactor">
  <h3>Hello, new cheff!</h3>
  <h4>Join our kitchen:</h4>
  <%= form_for(@chef) do |f| %>
    <%= f.text_field :LASTNAME,     :placeholder => "Your name and lastname" %> </br>
    <%= f.select :restaurant_id, restaurant_select, class: "select", :prompt => "Choose jobplace"%>
    <%= f.submit "Save information" %>
  <% end %>
</div>
